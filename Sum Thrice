list = []

list.append(int(input("What is your first number?")))
list.append(int(input("What is your second number?")))
list.append(int(input("What is your final number?")))


def calc(list):
    if list[0] == list[1] == list[2]:
        add_them = list[0] + list[1] + list [2]
        return add_them * 3
    else:
        add_them = list[0] + list[1] + list [2]
        return add_them

print(calc(list))
